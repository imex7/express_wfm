<h2>List</h2>

{{#if list.length}}
{{#each list}}
<div class="row">
	<div class="col s12 m7">
		<div class="card">
			<div class="card-image">
				<img src="{{img}}">
			</div>
			<div class="card-content">
				<span class="card-title">{{title}}</span>
				<p>Цена: {{price}}</p>
			</div>
			<div class="card-action">
				<a href="/list/{{id}}">Открыть</a>
			</div>
		</div>
	</div>
</div>
{{/each}}
{{else}}
<p>Nothing</p>
{{/if}}